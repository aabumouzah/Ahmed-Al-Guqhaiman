@article{Liu2016,
abstract = {{\textcopyright} 2016 ACM. The harsh environment in the water has imposed challenges for underwater sensor networks (USNs), which collect the sensed data from the underwater sensors to the sink on land. The time-varying underwater acoustic channel has low bandwidth and high bit error rate, which leads to low data collection efficiency. Furthermore, the heterogeneous model of USNs that uses acoustic communications under the water and wireless communication above the water makes it difficult in efficient routing and forwarding for data collection. To this end, we propose a novel on-surface wireless-assisted opportunistic routing (SurOpp) for USNs. SurOpp deploys multiple buoy nodes on surface and includes all of them in the forwarding candidates to form a receive diversity. The opportunities of reception and forwarding in buoy nodes are exploited to improve the end-to-end transmissions. SurOpp also adopts rateless codes in the source to achieve opportunistic reception in the sink. The cooperation of both opportunistic reception in the buoys and the sink further decreases the messages of control overhead. The wireless interface in the buoy undertakes all the message exchanges in forwarding coordination to compensate the bandwidth limit of the acoustic channel. Simulations in NS3 show that SurOpp outperforms the traditional routing and existing opportunistic routing in terms of packet delivery ratio, end-to-end delay and energy consumption.},
author = {Liu, Miaomiao and Ji, Fei and Guan, Quansheng and Yu, Hua and Chen, Fangjiong and Wei, Gang},
doi = {10.1145/2999504.3001102},
file = {:C$\backslash$:/Users/Ahmed/Desktop/New Papers/CS 6000/2016/Data processing, localization and security/On-surface wireless-assisted opportunistic routing for underwater sensor networks.pdf:pdf},
isbn = {9781450346375},
journal = {Proceedings of the 11th ACM International Conference on Underwater Networks {\&} Systems - WUWNet '16},
mendeley-groups = {PhD Research/Fall 2018/Week 2 {\&} 3},
pages = {1--5},
title = {{On-surface wireless-assisted opportunistic routing for underwater sensor networks}},
url = {http://dl.acm.org/citation.cfm?doid=2999504.3001102},
year = {2016}, note= {\textbf{Category:} Scan, \textbf{Timing:} 1 minute and 56 seconds. \textbf{Summary:} This paper proposed an opportunistic routing with multiple buoys to increase the reliability of acoustic links. So, an underwater node can transmit data via several buoys. This means a node will select the most efficient buoy, which provides a great enhancement in terms of packet delivery ratio, latency, and energy consumption., \textbf{Decision:} Scan. The author proposed a great routing protocol where multiple buoys should cooperate to optimize routing functions.}
}
