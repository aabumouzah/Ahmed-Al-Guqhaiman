@article{Rahman2017,
abstract = {{\textcopyright} 2017 Copyright held by the owner/author(s). Due to the atypical characteristics of the physical media in underwater acoustic sensor networks (UW-ASN)-mostly because of long propagation delay, low bandwidth and high error rate-several challenges arise while designing the MAC protocol. In this paper, we propose a Bidirectional Multi-flow MAC protocol (BMF-MAC) for UW-ASN, to efficiently handle multi-hop multi-flow traffic load patterns such a way that multiple streams of data transmissions concurrently proceed while adapting with varying traffic condition. BMF-MAC supports constitution of multi-hop flows by considering all pending packets in routing layer buffer and all flow setups requests from neighbors when setting up a flow, contrary to other underwater MAC protocols. The proposed MAC introduces a data transmission technique using the bidirectional multi-flow packet method for sending multiple data packets of the same flow in the reverse direction and thus improve channel utilization. The protocol is aimed to schedule more data transmission over multiple multihop flows, thus permitting rapid distribution of data and reduction of latency. Results show that BMF-MAC protocol outperforms existing CMRT protocol in terms of network throughput and packet delivery latency.},
author = {Rahman, J. and Shawkat, S.A. and Alam, M.S. and Hoque, M.A.},
doi = {10.1145/3148675.3148708},
file = {:C$\backslash$:/Users/Ahmed/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Rahman et al. - 2017 - BMF-MAC A bidirectional multi-flow MAC protocol for multihop underwater acoustic sensor networks.pdf:pdf},
isbn = {9781450355612},
journal = {Proceedings of the 12th ACM International Conference on Underwater Networks {\&} Systems - WUWNet '17},
keywords = {Handshake based MAC,Medium Access Control Protocols,Under Water Acoustic Sensor Network (UW-ASN)},
mendeley-groups = {PhD Research/Summer 2018,PhD Research/Summer 2018/Week 8,PhD Research/UWSN Dissertation/Underwater MAC,PhD Research/Fall 2018/Week 2 {\&} 3,PhD Research/Fall 2018/Week 2 {\&} 3/Scan},
pages = {4--11},
title = {{BMF-MAC: A bidirectional multi-flow MAC protocol for multihop underwater acoustic sensor networks}},
year = {2017},
title = {{BMF-MAC: A bidirectional multi-flow MAC protocol for multihop underwater acoustic sensor networks}},
year = {2017}, note= {\textbf{Category:} Best, \textbf{Timing:} 1 minute and 41 seconds. \textbf{Summary:} This paper proposed a MAC protocol that allows multiple data packets to be sent at the same time. It also enable sending multiple packets in the reverse direction, which increases channel utilization. The result of this paper showed that the proposed work minimize delay while increasing packet delivery ratio compared to the Cascading Multihop Reservation and Transmission (CMRT) protocol., \textbf{Decision:} Scan. It is great to enable full duplex mode to exchange data. This helps to reduce the latency and extend network lifetime.}
