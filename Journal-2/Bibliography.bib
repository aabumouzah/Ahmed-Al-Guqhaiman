@article{Liu2017,
abstract = {This paper presents a new entropy minimization criterion and corresponding algorithms that are used for both symbol timing and carrier frequency recovery for underwater acoustic receivers. It relies on the entropy estimation of the eye diagram and the constellation diagram of the received signal. During the parameter search, when perfect synchronization is achieved, the entropy will reach a global minimum, indicating the least intersymbol interference or a restored constellation diagram. Unlike other synchronization methods, this unified criterion can be used to build an all-in-one synchronizer with high accuracy. The feasibility of this method is proven using a theoretical analysis and supported by sea trial measurement data.},
author = {Liu, X. and Bousquet, J.},
doi = {10.1145/3148675.3148719},
